{
  "query": {
    "_ttl": {
      "enabled": true,
      "default": "14d"
    },
    "dynamic": "strict",
    "properties": {
      "id": {
        "type": "keyword"
      },
      "text": {
        "type": "string"
      },
      "profile": {
        "type": "keyword"
      },
      "sortDate": {
        "type": "date",
        "format": "epoch_millis"
      },
      "suggest": {
        "type": "completion",
        "index_analyzer": "simple",
        "search_analyzer": "simple",
        "payloads": true
      }
    }
  }
}
